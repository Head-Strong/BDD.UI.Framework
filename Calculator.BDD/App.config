<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="specFlow" type="TechTalk.SpecFlow.Configuration.ConfigurationSectionHandler, TechTalk.SpecFlow" />
  </configSections>



  <appSettings>
    <add key="DriverExe" value="G:\Chrome\chromedriver" />
    <add key="PortableExe" value="G:\Chrome\Portable\GoogleChromePortable.exe" />
    <add key="DefaultProfile" value="G:\Chrome\Portable\Data\profile\Default" />
    <add key="ScreenShotLocation" value="Result" />
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
  </startup>



  <specFlow>
    <!-- For additional details on SpecFlow configuration options see http://go.specflow.org/doc-config -->
    <!-- For additional details on SpecFlow configuration options see http://go.specflow.org/doc-config -->
    <unitTestProvider name="NUnit" />
  </specFlow>
</configuration>
